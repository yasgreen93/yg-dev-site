    "express-validator": "^5.0.1",
    "html-to-text": "^3.3.0",
    "juice": "^4.2.2",
    "morgan": "~1.6.1",
    "nodemailer": "^4.6.0",
    "pug": "^2.0.0-rc.4",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
